<dashboard xmlns:ns0="http://ns.adobe.com/textLayout/2008" id="dashboard2">
  <mugloverrides>
    <element name="window">
      <attribute name="margin" value="1" />
      <attribute name="border" value="2" />
      <attribute name="padding" value="3" />
      <element name="trim">
        <attribute name="color" value="brown" />
      </element>
      <element name="pane" action="empty"/>
    </element>
    <element name="plot">
      <elements name="datatips">
      	<attribute name="border" value="1" />
        <attribute name="bgcolor" value="0x33383b" />
        <attribute name="bordercolor" value="0x5b6473" />
        <attribute name="fontcolor" value="0xffffff" />
        <attribute name="fontsize" value="14" />
        <attribute name="bold" value="true" />
        <attribute name="pad" value="2" />
      </elements>
    </element>
  </mugloverrides>

  <tab id="climateChange2">
    <graph id="temperature2" name="Temperature">
      <mugl>
        <window border="0" margin="0" padding="0">
          <pane>clear</pane>
        </window>
        <plot>
          <datatips/>
        </plot>
        <plot>
          <datatips/>
        </plot>
      </mugl>
    </graph>
  </tab>
</dashboard>
